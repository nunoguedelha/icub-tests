<application>
    <name>iCub robot</name>
    <description>A fixture to check that iCub robot encoder ports are available.</description>
    <version>0.1</version>
    <authors>
        <author email="nuno.guedelha@iit.it">Nuno Guedelha</author>
    </authors>
    <module>
        <name>yarp</name>
        <parameters>help</parameters>
        <node>localhost</node>
        <prefix></prefix>
        <dependency>
            <port timeout="10">/${robotname}/left_leg/stateExt:o</port>
            <port timeout="10">/${robotname}/right_leg/stateExt:o</port>
            <port timeout="10">/${robotname}/left_arm/stateExt:o</port>
            <port timeout="10">/${robotname}/right_arm/stateExt:o</port>
            <port timeout="10">/${robotname}/torso/stateExt:o</port>
        </dependency>
    </module>

</application>

